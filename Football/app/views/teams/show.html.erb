
<h1><%=@team.name%></h1>

<% @team.players.each do |t| %>

    <p>Player Name: <%=t.name %></p>
    <p>Player Position: <%=t.position %></p>
    <p>Player Starts?: <%=t.starter %></p>
    <p>Player Fantasy Points: <%=t.fantasy_points %></p><br>
    <br>

<%end%>


<p><%=link_to "Delete Team", league_team_path(@team), method: :delete %><p>

<%=link_to "Go Back to League", user_leagues_path(@league, @team) %>